{
  "cli": {
    "version": ">= 16.18.0",
    "appVersionSource": "remote"
  },
  "build": {
    "development": {
      "node": "20.19.4",
      "android": {
        "buildType": "apk"
      },
      "env": {
        "EXPO_NO_CACHE": "1"
      }
    },
    "preview": {
      "node": "20.19.4",
      "android": {
        "buildType": "apk"
      },
      "env": {
        "EXPO_NO_CACHE": "1"
      }
    },
    "production": {
      "autoIncrement": true,
      "distribution": "store",
      "node": "20.19.4",
      "android": {
        "buildType": "app-bundle"
      },
      "env": {
        "EXPO_NO_CACHE": "1"
      }
    }
  },
  "submit": {
    "production": {
      "android": {
        "serviceAccountKeyPath": "./credentials.json",
        "track": "production"
      }
    }
  }
}